# MinIO Configuration
#MINIO_ENDPOINT=localhost:9000
#MINIO_ACCESS_KEY=minioadmin
#MINIO_SECRET_KEY=minioadmin
#MINIO_SECURE=false

# OCR Configuration
OCR_DEFAULT_LANGUAGE=id

# API Keys Authentication
# Set to true to require API keys for OCR endpoints
API_KEYS_ENABLED=false
# Comma-separated list of static API keys (optional, can use database instead)
API_KEYS=sk-ocr-2fdb554af5cb835d0a64518cd00deceb

# Admin Master Key - USE THIS FOR INITIAL SETUP
# Generate your own secure key for production!
ADMIN_MASTER_KEY=ba8281f4c5bf95137130c292e7807b260d6794c39adf0651c1816beba70bf6c3

# Rate Limiting
RATE_LIMIT_ENABLED=true
RATE_LIMIT_PER_MINUTE=100

# Performance Tuning (untuk optimasi response time)
# PDF DPI - 150 lebih cepat, 200 lebih akurat (default: 150)
PDF_DPI=300
# Default enhance - aktifkan preprocessing untuk dokumen jadul/pudar (default: false)
DEFAULT_ENHANCE=true
# Angle classification - matiin kalo dokumen udah lurus biar lebih cepet (default: false)
USE_ANGLE_CLS=false
# Max image dimension - gambar lebih gede akan di-resize otomatis (default: 2000)
MAX_IMAGE_DIMENSION=2000
# Parallel PDF processing - proses halaman PDF bersamaan (default: true)
PARALLEL_PDF_PROCESSING=false
# Jumlah worker untuk parallel processing (default: 4)
PDF_WORKERS=2

# Data directory (untuk Docker volume)
DATA_DIR=/app/data